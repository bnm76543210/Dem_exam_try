<Window x:Class="Preddiplom_practice.Windows.CAPTCHA"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Preddiplom_practice.Windows"
        mc:Ignorable="d"
        Title="CAPTCHA" Height="250" Width="250" ResizeMode="CanMinimize">
    <Grid>
        <Label Content="Введите код с картинки" Margin="0,0,-0.4,0" Width="194" VerticalAlignment="Top" VerticalContentAlignment="Center" HorizontalContentAlignment="Center"/>
        <TextBox x:Name="captchaTextBox" Height="23" Margin="50,0,0,65" TextWrapping="Wrap" Width="150" VerticalContentAlignment="Center" VerticalAlignment="Bottom" HorizontalAlignment="Left" KeyDown="captchaTextBox_KeyDown"/>
        <Label x:Name="Captcha" Content="Label" HorizontalAlignment="Left" Margin="100,71,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
